<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="gridPane" alignment="CENTER" prefHeight="50.0" stylesheets="@../css/home.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label fx:id="name" text="Project Name" textFill="WHITE" GridPane.rowIndex="1">
            <padding>
                <Insets left="5.0" />
            </padding></Label>
        <Label fx:id="start_date" text="Start Date" textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="5.0" />
            </GridPane.margin></Label>
        <Label fx:id="customer" text="Customer" textFill="WHITE" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <padding>
                <Insets left="5.0" />
            </padding></Label>
        <Label fx:id="status" styleClass="item_progress_label" stylesheets="@../css/home.css" text="Status" textFill="WHITE" GridPane.columnIndex="3" GridPane.rowIndex="1">
            <padding>
                <Insets bottom="6.0" left="8.0" right="8.0" top="6.0" />
            </padding>
        </Label>
        <Label fx:id="note" text="Notes" textFill="WHITE" GridPane.columnIndex="5" GridPane.rowIndex="1" />
        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="4" GridPane.rowIndex="1">
            <children>
                <ImageView fitHeight="20.0" fitWidth="20.0" layoutX="4.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../icons/workers_orange.png" />
                    </image>
                </ImageView>
                <Label fx:id="note1" layoutX="31.0" layoutY="17.0" text="Team" textFill="WHITE" />
            </children>
        </AnchorPane>
    </children>
</GridPane>
