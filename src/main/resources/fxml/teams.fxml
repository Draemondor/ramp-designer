<BorderPane fx:id="rootPane" styleClass="root_borderPane" stylesheets="@../css/home.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TeamsController">
   <top>
      <VBox prefHeight="160.0" prefWidth="950.0" stylesheets="@../css/home.css" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="125.0" prefWidth="200.0">
               <children>
                  <AnchorPane HBox.hgrow="ALWAYS">
                     <children>
                        <ImageView fx:id="backBtn" fitHeight="30.0" fitWidth="30.0" layoutX="44.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="44.0" AnchorPane.topAnchor="45.0">
                           <image>
                              <Image url="@../icons/back_arrow.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
                  <AnchorPane layoutX="10.0" layoutY="10.0" HBox.hgrow="ALWAYS">
                     <children>
                        <TextField fx:id="searchBar" focusTraversable="false" layoutX="105.0" layoutY="45.0" prefHeight="35.0" prefWidth="350.0" promptText="Search..." stylesheets="@../css/home.css" AnchorPane.leftAnchor="80.0" AnchorPane.rightAnchor="80.0" AnchorPane.topAnchor="45.0">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </TextField>
                     </children>
                  </AnchorPane>
                  <AnchorPane layoutX="210.0" layoutY="10.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Button fx:id="newTeamBtn" layoutX="252.0" layoutY="55.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="125.0" styleClass="_button" stylesheets="@../css/home.css" text="New Team" textFill="WHITE" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="45.0">
                           <font>
                              <Font name="System Bold" size="15.0" />
                           </font>
                        </Button>
                     </children>
                  </AnchorPane>
               </children>
            </HBox>
            <GridPane stylesheets="@../css/home.css">
               <columnConstraints>
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints vgrow="ALWAYS" />
                  <RowConstraints />
               </rowConstraints>
               <children>
                  <Label text="Team Name" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                     <padding>
                        <Insets right="30.0" />
                     </padding>
                  </Label>
                  <Label text="Projects" textFill="WHITE" GridPane.columnIndex="1">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                  </Label>
                  <Label text="Members" textFill="WHITE" GridPane.columnIndex="2">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                  </Label>
                  <Label text="Privacy" textFill="WHITE" GridPane.columnIndex="3">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets left="35.0" right="35.0" />
               </VBox.margin>
               <padding>
                  <Insets left="10.0" right="22.0" />
               </padding>
            </GridPane>
         </children>
      </VBox>
   </top>
   <center>
      <VBox fx:id="items" stylesheets="@../css/home.css" BorderPane.alignment="CENTER">
         <children>
            <AnchorPane style="-fx-background-color: #41403D;" stylesheets="@../css/home.css" VBox.vgrow="ALWAYS">
               <children>
                  <ListView fx:id="listView" stylesheets="@../css/home.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
         </children>
         <BorderPane.margin>
            <Insets bottom="35.0" left="35.0" right="35.0" />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
